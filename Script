local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Town Script by Ike#5195", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Plot Copier",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/TownScript/main/PlotCopier?t='..tostring(tick())))()
  	end    
})

Tab:AddButton({
	Name = "Invisible Tool",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/TownScript/main/InvisibleTool'))()
  	end    
})

local Tab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Gun Mod",
	Callback = function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/TownScript/main/GunMod'))()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/TownScript/main/GunModRespawn'))()
  	end    
})

